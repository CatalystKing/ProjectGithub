<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SeeApt"
    android:background="@color/babyblue">

    <TextView
        android:id="@+id/appp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="View Appointment"
        android:textColor="@color/black"
        android:textSize="30dp"
        app:drawableTopCompat="@drawable/ic_baseline_preview_24" />

    <TextView
        android:id="@+id/myNametxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/appp"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="0dp"
        android:fontFamily="@font/antic_slab"
        android:text="Name:"
        android:textColor="#000000"

        android:textSize="18sp"
        app:drawableStartCompat="@drawable/ic_baseline_drive_file_rename_outline_24" />

    <TextView
        android:id="@+id/myName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/myNametxt"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/antic_slab"
        android:text="Name"
        android:textColor="#000000"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/myEmailtxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/myName"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:drawableStart="@drawable/ic_baseline_alternate_email_24"
        android:fontFamily="@font/antic_slab"
        android:text="Email:"
        android:textColor="#000000"

        android:textSize="18sp" />

    <TextView
        android:id="@+id/myEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/myEmailtxt"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/antic_slab"
        android:text="Email"
        android:textColor="#000000"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/myPhonetxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/myEmail"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/antic_slab"
        android:text="Contact me @"
        android:textColor="#000000"
        android:textSize="18sp"
        app:drawableStartCompat="@drawable/ic_baseline_local_phone_24" />

    <TextView
        android:id="@+id/myPhone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/myPhonetxt"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/antic_slab"
        android:text="00000000"
        android:textColor="#000000"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/myDatetxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/myPhone"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:drawableStart="@drawable/ic_baseline_date"
        android:fontFamily="@font/antic_slab"

        android:text="Date of Appointment:"
        android:textColor="#000000"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/myDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/myDatetxt"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/antic_slab"
        android:text="Not Available"
        android:textColor="#000000"
        android:textSize="18sp"
        tools:ignore="UnknownId" />

    <TextView
        android:id="@+id/myTimetxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/myDate"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:drawableStart="@drawable/ic_baseline_access_time_filled_24"
        android:fontFamily="@font/antic_slab"
        android:text="Time of Appointment:"
        android:textColor="#000000"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/myTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/myTimetxt"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/antic_slab"
        android:text="Not Available"
        android:textColor="#000000"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/myTypetxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/myTime"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:drawableStart="@drawable/ic_baseline_playlist_add_check_circle_24"
        android:fontFamily="@font/antic_slab"
        android:text="Type of Check up:"
        android:textColor="#000000"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/myType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/myTypetxt"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/antic_slab"
        android:text="Not Available"
        android:textColor="#000000"
        android:textSize="18sp" />


</RelativeLayout>